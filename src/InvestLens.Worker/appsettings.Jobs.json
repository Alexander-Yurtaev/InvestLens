{
  "HangfireJobs": {
    "RecurringJobs": [
      {
        "JobId": "securities.daily-refresh",
        "ServiceType": "InvestLens.Worker.Services.ISecuritiesService",
        "MethodName": "ProcessDailyDataRefreshAsync",
        "CronExpression": "0 2 * * *",
        "TimeZone": "Russian Standard Time",
        "Queue": "daily",
        "Enabled": false,
        "Description": "Ежедневное обновление: Securities."
      },
      {
        "JobId": "dictionaries.daily-refresh",
        "ServiceType": "InvestLens.Worker.Services.IGlobalIssDictionariesService",
        "MethodName": "ProcessDailyDataRefreshAsync",
        "CronExpression": "0 2 * * *",
        "TimeZone": "Russian Standard Time",
        "Queue": "daily",
        "Enabled": false,
        "Description": "Ежедневное обновление: Global ISS Dictionaries."
      }
    ],
    "StartupJobs": [
      {
        "JobId": "securities.app-initialization",
        "ServiceType": "InvestLens.Worker.Services.ISecuritiesService",
        "MethodName": "InitializeApplicationAsync",
        "DelaySeconds": 60,
        "Queue": "critical",
        "Enabled": false,
        "Description": "Инициализация: Securities."
      },
      {
        "JobId": "dictionaries.app-initialization",
        "ServiceType": "InvestLens.Worker.Services.IGlobalIssDictionariesService",
        "MethodName": "InitializeApplicationAsync",
        "DelaySeconds": 60,
        "Queue": "critical",
        "Enabled": true,
        "Description": "Инициализация: Global ISS Dictionaries."
      }
    ]
  }
}
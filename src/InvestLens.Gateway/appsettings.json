{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GrpcMoexServerAddress": "https://investlens.data.api:8081",
  "ReverseProxy": {
    "Routes": {
      "data-service-route": {
        "ClusterId": "data-service",
        "Match": {
          "Path": "/api/data/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "data-service": {
        "Destinations": {
          "destination1": {
            "Address": "https://investlens.data.api:8081"
          }
        },
        "HttpClient": {
          "SslProtocols": [ "Tls12", "Tls13" ]
        }
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/app-.log",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
